{
    "$schema": "http://dev.vanilla.localhost/api/v2/layouts/schema?layoutViewType=categoryList&cacheBuster=5",
    "dateInserted": "2023-08-13T00:00:00Z",
    "layoutViewType": "categoryList",
    "layoutID": "categoryList",
    "name": "Default Category List Layout",
    "layout": [
        {
            "$hydrate": "react.section.full-width",
            "children": [
                {
                    "$hydrate": "react.app-banner",
                    "showTitle": true,
                    "showDescription": true,
                    "showSearch": true
                }
            ]
        },
        {
            "$hydrate": "react.section.2-columns",
            "breadcrumbs": [
                {
                    "$hydrate": "react.breadcrumbs"
                }
            ],
            "mainBottom": [
                {
                    "$hydrate": "react.asset.categoryFollow"
                },
                {
                    "$hydrate": "react.asset.categoryList",
                    "apiParams": {
                        "filter": "none"
                    },
                    "title": "Categories",
                    "isAsset": true,
                    "categoryOptions": {
                        "description": {
                            "display": true
                        },
                        "followButton": {
                            "display": true
                        },
                        "metas": {
                            "display": {
                                "discussionCount": true,
                                "commentCount": true,
                                "postCount": true,
                                "followerCount": true,
                                "lastPostName": true,
                                "lastPostAuthor": true,
                                "lastPostDate": true,
                                "subcategories": true
                            }
                        }
                    }
                }
            ],
            "secondaryBottom": [
                {
                    "$hydrate": "react.guest-cta",
                    "title": "Welcome!",
                    "description": "It looks like you're new here. Sign in or register to get started.",
                    "secondButton": {
                        "title": "Register",
                        "type": "standard"
                    },
                    "button": {
                        "title": "Sign In",
                        "type": "primary"
                    },
                    "background": {
                        "useOverlay": true
                    }
                },
                {
                    "$hydrate": "react.newpost",
                    "asOwnButtons": [],
                    "excludedButtons": [],
                    "postableDiscussionTypes": ["discussion", "question", "poll"]
                },
                {
                    "$hydrate": "react.quick-links",
                    "title": "Quick Links"
                }
            ]
        }
    ]
}
